<configuration>
  <system.webServer>
    <staticContent>
      <remove fileExtension=".json" />
      <remove fileExtension=".obj" />
      <mimeMap fileExtension=".json" mimeType="application/json" />
      <mimeMap fileExtension=".obj"  mimeType="application/object" />
    </staticContent>
  </system.webServer>
</configuration>